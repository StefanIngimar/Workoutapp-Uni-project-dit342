							"    pm.response.to.have.status(201);\r",
							"});\r",
							"\r",
							"var jsonData = pm.response.json();\r",
							"var sentData = JSON.parse(pm.request.body.raw);\r",
							"\r",
							"pm.globals.set(\"sessionId\", jsonData._id);\r",
							"\r",
							"// Test the top-level keys in the response\r",
							"pm.test(\"Correct response structure\", function() {\r",
							"    pm.expect(jsonData).to.have.property(\"_id\");\r",
							"    pm.expect(jsonData).to.have.property(\"userID\").that.equals(sentData.userID);\r",
							"    pm.expect(jsonData).to.have.property(\"sessionName\").that.equals(sentData.sessionName);\r",
							"    pm.expect(jsonData).to.have.property(\"duration\").that.equals(sentData.duration);\r",
							"    pm.expect(jsonData).to.have.property(\"isCompleted\").that.equals(sentData.isCompleted);\r",
							"    pm.expect(jsonData).to.have.property(\"notes\").that.equals(sentData.notes);\r",
							"});\r",
							"\r",
							"pm.test(\"Correct exercise data\", function() {\r",
							"    pm.expect(jsonData.exercises).to.be.an(\"array\").that.has.lengthOf(sentData.exercises.length);\r",
							"    \r",
							"    jsonData.exercises.forEach(function(exercise, index) {\r",
							"        var sentExercise = sentData.exercises[index];\r",
							"        pm.expect(exercise.name).to.eql(sentExercise.name);\r",
							"        pm.expect(exercise.sets).to.eql(sentExercise.sets);\r",
							"        pm.expect(exercise.reps).to.eql(sentExercise.reps);\r",
							"        pm.expect(exercise.weight).to.eql(sentExercise.weight);\r",
							"    });\r",
							"});"
						],
						"type": "text/javascript",
						"packages": {}
					}
				}
			],
